<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.crm.dao.CustomerDao">
    <insert id="add"  parameterType="com.crm.entity.Customer">
        insert into tbl_customer (
            id,owner,name,website,phone,contactSummary,nextContactTime,description,address
        ) values (#{id},#{owner},#{name},#{website},#{phone},#{contactSummary},#{nextContactTime},#{description},#{address})
    </insert>
</mapper>